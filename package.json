  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/win11-pwa.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/win11-pwa/issues"
  },
  "homepage": "https://yourusername.github.io/win11-pwa/",
  "devDependencies": {
    "serve": "^14.2.0",
    "live-server": "^1.2.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
