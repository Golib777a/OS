    "test": "echo 'No tests configured'"
  },
  "keywords": [
    "windows",
    "windows-11",
    "pwa",
    "progressive-web-app",
    "desktop-simulation",
    "web-os",
    "html5",
    "css3",
    "javascript"
  ],
  "author": "Your Name",
